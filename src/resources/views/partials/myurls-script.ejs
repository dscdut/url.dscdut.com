<script>
    $(document).ready(function () {
        $("#checkbox-all").change(function () {
            if (this.checked) {
                $("#checkbox-item").each(function() {
                    this.checked=true;
                })
            } else {
                $("#checkbox-item").each(function() {
                    this.checked=false;
                })
            }
        })
        $("#myurls-edit-icon").click(function () {
            $("#myurls-item").hide()
            $("#myurls-item-edit").show()
        })
        $("#myurls-save-icon").click(function () {
            $("#myurls-item").show()
            $("#myurls-item-edit").hide()
        })
        $("#myurls-cancel-icon").click(function () {
            $("#myurls-item").show()
            $("#myurls-item-edit").hide()
        })
    })
</script>